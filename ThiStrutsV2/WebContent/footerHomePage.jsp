<script>
	function setLai() {
		var sel = document.getElementById('cbCPU');
		sel.selectedIndex = 0;
		var sel2 = document.getElementById('cbHangSanPham');
		sel2.selectedIndex = 0;
		ajaxHome('9999');
	}
	function kiemTra() {
		var arrChkBox1 = document.getElementById("50").value;
		if (arrChkBox1 <= 0) {
			var arrChkBox2 = document.getElementById("50");
			$(arrChkBox2).hide();
		}
	}
	function ajaxHome(soTrang) {
		var txtCPU = document.getElementById("cbCPU").value;
		var txtTimKiem = document.getElementById("txtTimKiem").value;
		var txtHangSanPham = document.getElementById("cbHangSanPham").value;
		var tam = 4;
		var xmlhttp;
		if (window.XMLHttpRequest) {
			xmlhttp = new XMLHttpRequest();
		} else {
			xmlhttp = new ActiveXObject("Microsoft.XMLHTTP");
		}
		xmlhttp.onreadystatechange = function() {
			if (xmlhttp.readyState == 4 && xmlhttp.status == 200) {
				if (soTrang == 9999) {
					document.getElementById("asd").innerHTML = xmlhttp.responseText;
				} else {
					var arrChkBox1 = document.getElementsByName("buttonAB");
					$(arrChkBox1).hide();
					document.getElementById("asd").innerHTML = document
							.getElementById("asd").innerHTML
							+ xmlhttp.responseText;
				}
				if (soTrang == 9999) {
					tam = document.getElementById("50").value;
				}
				if (soTrang == 9999 && tam <= 0) {
					var arrChkBox1 = document.getElementsByName("buttonAB");
					$(arrChkBox1).hide();
				}
				if (soTrang <= 12) {
					var arrChkBox1 = document.getElementsByName("buttonAB");
					$(arrChkBox1).hide();
				}
				if (tam <= -12 || soTrang <= 0) {
					document.getElementById("asd").innerHTML = "No Product";
				}
			}
		}
		xmlhttp.open("GET",
				"http://localhost:8080/ThiStrutsV2/home-page.do?soTrang="
						+ soTrang + "&txtTimKiem=" + txtTimKiem + "&maHang="
						+ txtHangSanPham + "&tenCPU=" + txtCPU + "", true);
		xmlhttp.send();
	}
</script>
